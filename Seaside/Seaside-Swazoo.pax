	classInstanceVariableNames: ''!
WorkingHTTPSetCookieField subclass: #WorkingHTTPSetCookie2Field
	instanceVariableNames: ''
	classVariableNames: ''
	poolDictionaries: ''
	classInstanceVariableNames: ''!
SwazooSite subclass: #WAPluggableSite
	instanceVariableNames: 'handler'
	classVariableNames: ''
	poolDictionaries: ''
	classInstanceVariableNames: ''!

"Global Aliases"!


"Loose Methods"!

!GRPackage class methodsFor!

seasideSwazoo
	^ self new
		name: 'Seaside-Swazoo';
		description: 'A small set of Swazoo 2 extensions to make it run Seaside.';
		addDependency: 'Grease-Core';
		url: #seasideLGPLUrl;
		license: #LGPL;
		yourself
! !
