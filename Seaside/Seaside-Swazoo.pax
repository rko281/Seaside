| package |
package := Package name: 'Seaside-Swazoo'.
package paxVersion: 1;
	basicComment: ''.

package basicPackageVersion: '19'.


package classNames
	add: #WAPluggableSite;
	add: #WorkingHTTPSetCookie2Field;
	add: #WorkingHTTPSetCookieField;
	yourself.

package methodNames
	add: 'GRPackage class' -> #seasideSwazoo;
	yourself.

package binaryGlobalNames: (Set new
	yourself).

package globalAliases: (Set new
	yourself).

package setPrerequisites: (IdentitySet new
	add: '..\..\Grease\Grease-Core';
	add: '..\..\Swazoo\Swazoo';
	yourself).

package!

