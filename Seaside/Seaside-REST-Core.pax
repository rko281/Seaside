	add: #WAMultiPlaceholderPathMatch;
	add: #WAOrPathMatch;
	add: #WAParameterMatch;
	add: #WAPlaceholderParameterMatch;
	add: #WAPlaceholderPathMatch;
	add: #WAPPathMatch;
	add: #WAPrefixPlaceholderPathMatch;
	add: #WARestfulComponentFilter;
