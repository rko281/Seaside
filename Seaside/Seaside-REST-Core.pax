	basicComment: ''.


package classNames
	add: #WAArgumentNotFoundError;
	add: #WAByMethodRoutes;
	add: #WAComplexRoute;
	add: #WACompositeMimeTypeMatch;
	add: #WAExactParameterMatch;
	add: #WAExactPathMatch;
	add: #WAFullMimeTypeMatch;
	add: #WAFullPlaceholderPathMatch;
	add: #WAMainMimeTypeMatch;
	add: #WAMatch;
