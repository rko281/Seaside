	basicComment: ''.


package classNames
	add: #WAWelcomeTest;
	yourself.

package methodNames
	add: 'GRPackage class' -> #seasideTestsWelcome;
	yourself.

package binaryGlobalNames: (Set new
	yourself).

package globalAliases: (Set new
	yourself).

package setPrerequisites: #(
	'..\..\Core\Object Arts\Dolphin\Base\Dolphin'
	'..\..\Grease\Grease-Core'
	'Seaside-Tools-Core'
	'Seaside-Welcome'
	'..\..\Core\Contributions\Camp Smalltalk\SUnit\SUnit').

package!

"Class Definitions"!

TestCase subclass: #WAWelcomeTest
	instanceVariableNames: ''
	classVariableNames: ''
	poolDictionaries: ''
	classInstanceVariableNames: ''!

"Global Aliases"!


"Loose Methods"!

!GRPackage class methodsFor!

seasideTestsWelcome	^ self new		name: 'Seaside-Tests-Welcome';		description: 'Unit tests for the welcome screen.';		addDependency: 'Seaside-Welcome';		url: #seasideUrl;		yourself! !
!GRPackage class categoriesFor: #seasideTestsWelcome!*seaside/tests/welcome!public! !

