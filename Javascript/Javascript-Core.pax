!Object methodsFor!

asFunction	^ self asFunction: #()!

asFunction: aCollection	^ JSFunction new add: self; arguments: aCollection!

asJavascript	^ JSJavascriptCanvas builder render: [ :javascript |		  self javascriptOn: javascript ]!

asJavascriptOn: aStream	^ JSJavascriptCanvas builder		  render: [ :javascript | self javascriptOn: javascript ]		  on: aStream!

isJavascript	^ false!

javascriptOn: aRenderer	self printOn: aRenderer document stream! !

